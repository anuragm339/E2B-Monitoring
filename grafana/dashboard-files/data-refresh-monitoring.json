      "id": 8,
      "title": "\u26a0\ufe0f In Progress Topics (Not Yet Completed)",
      "type": "table",
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus-datasource"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-datasource"
          },
          "expr": "data_refresh_start_time_seconds unless data_refresh_end_time_seconds > 0",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "instance": true,
              "job": true,
              "service": true,
              "__name__": true,
              "Value": true
            },
            "indexByName": {
              "refresh_id": 0,
              "topic": 1,
              "refresh_type": 2
            },
            "renameByName": {
              "refresh_id": "Batch ID",
              "topic": "Topic",
              "refresh_type": "Type"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Topic",
            "desc": false
          }
        ]
      }
    },
